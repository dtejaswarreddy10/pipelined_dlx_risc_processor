/*
MEM/WB Pipelibe Registers 
*/


module mem_wb_regs (
	input clk,reset,

        input [31:0] lmd_in,
        input [31:0] aluoutput_in,
        input [4:0] rd_in,

	input reg_read_in,
        input mem_to_reg_in,

        output [31:0] lmd_out,
        output [31:0] aluoutput_out,
        output [4:0] rd_out,


        output reg reg_read_out,
        output reg mem_to_reg_out
);
	always@(posedge clk or posedge reset) begin
		if (reset) begin

        		lmd_out		<= 'd0;
        		aluoutput_out	<= 'd0;
        		rd_out		<= 'd0;

        		reg_read_out	<= 'd0;
        		mem_to_reg_out	<= 'd0;

		end else begin
        		lmd_out		<= lmd_in;
        		aluoutput_out	<= aluoutput_in;
        		rd_out		<= rd_in;

        		reg_read_out	<= reg_read_in;
        		mem_to_reg_out	<= mem_to_reg_in;

		end

	end

endmodule
